{
  "name": "unifreefrontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "lint": "eslint ./src",
    "lint:fix": "eslint ./src",
    "test": " react-scripts test"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint",
      "pre-push": "set CI=true && npm run test "
    }
  },
  "dependencies": {
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-scripts": "3.1.1"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "babel-eslint": "^10.0.2",
    "eslint": "^6.4.0",
    "eslint-plugin-react": "^7.14.3",
    "husky": "^3.0.5"
  }
}